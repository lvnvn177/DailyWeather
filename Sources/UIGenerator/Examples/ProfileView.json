{
    "type": "stack",
    "id": "profileContainer",
    "properties": {
        "direction": "vertical",
        "spacing": 16
    },
    "padding": 20,
    "backgroundColor": "#FFFFFF",
    "children": [
        {
            "type": "image",
            "id": "profileImage",
            "properties": {
                "systemName": "person.circle.fill"
            },
            "frame": {
                "width": 120,
                "height": 120
            },
            "foregroundColor": "#007AFF"
        },
        {
            "type": "text",
            "id": "nameText",
            "properties": {
                "text": "John Doe"
            },
            "font": {
                "size": 24,
                "weight": "bold",
                "design": "rounded"
            }
        },
        {
            "type": "text",
            "id": "emailText",
            "properties": {
                "text": "john.doe@example.com"
            },
            "font": {
                "size": 16,
                "weight": "regular",
                "design": "default"
            },
            "foregroundColor": "#666666"
        },
        {
            "type": "stack",
            "id": "statsContainer",
            "properties": {
                "direction": "horizontal",
                "spacing": 30
            },
            "padding": 20,
            "backgroundColor": "#F5F5F5",
            "cornerRadius": 12,
            "children": [
                {
                    "type": "stack",
                    "id": "postsStack",
                    "properties": {
                        "direction": "vertical",
                        "spacing": 4
                    },
                    "children": [
                        {
                            "type": "text",
                            "id": "postsCount",
                            "properties": {
                                "text": "156"
                            },
                            "font": {
                                "size": 20,
                                "weight": "bold",
                                "design": "rounded"
                            }
                        },
                        {
                            "type": "text",
                            "id": "postsLabel",
                            "properties": {
                                "text": "Posts"
                            },
                            "font": {
                                "size": 14,
                                "weight": "regular",
                                "design": "default"
                            },
                            "foregroundColor": "#666666"
                        }
                    ]
                },
                {
                    "type": "stack",
                    "id": "followersStack",
                    "properties": {
                        "direction": "vertical",
                        "spacing": 4
                    },
                    "children": [
                        {
                            "type": "text",
                            "id": "followersCount",
                            "properties": {
                                "text": "1.2K"
                            },
                            "font": {
                                "size": 20,
                                "weight": "bold",
                                "design": "rounded"
                            }
                        },
                        {
                            "type": "text",
                            "id": "followersLabel",
                            "properties": {
                                "text": "Followers"
                            },
                            "font": {
                                "size": 14,
                                "weight": "regular",
                                "design": "default"
                            },
                            "foregroundColor": "#666666"
                        }
                    ]
                },
                {
                    "type": "stack",
                    "id": "followingStack",
                    "properties": {
                        "direction": "vertical",
                        "spacing": 4
                    },
                    "children": [
                        {
                            "type": "text",
                            "id": "followingCount",
                            "properties": {
                                "text": "384"
                            },
                            "font": {
                                "size": 20,
                                "weight": "bold",
                                "design": "rounded"
                            }
                        },
                        {
                            "type": "text",
                            "id": "followingLabel",
                            "properties": {
                                "text": "Following"
                            },
                            "font": {
                                "size": 14,
                                "weight": "regular",
                                "design": "default"
                            },
                            "foregroundColor": "#666666"
                        }
                    ]
                }
            ]
        },
        {
            "type": "button",
            "id": "editProfileButton",
            "properties": {
                "title": "Edit Profile"
            },
            "padding": 15,
            "backgroundColor": "#007AFF",
            "foregroundColor": "#FFFFFF",
            "cornerRadius": 8
        }
    ]
}
